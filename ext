import pandas as pd
import re

# Provided text
text = """
466
testtest
24.57
time 318.88
464
testtest
24.54
time 317.88
"""

# Regular expression pattern to extract the required information
pattern = r"(\d+)\s+[\w\s]+\s+(\d+\.\d+)\s+time:(\d+\.\d+)"

# Using regex to find all matches
matches = re.findall(pattern, text)

# Creating a DataFrame from the matches
df = pd.DataFrame(matches, columns=["length", "lift", "time"])
